
public abstract class FuncionarioAutenticavel extends Funcionario {

	private int senha; // inicialisa automaticamente com zero

	public void setSenha(int senha) {
		this.senha = senha;
	}

	public boolean autentica(int senha) {
		if (this.senha == senha) {
			return true;
		} else {
			return false;
		}
	}
}
