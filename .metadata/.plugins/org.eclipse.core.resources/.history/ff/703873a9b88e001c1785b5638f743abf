package testeNubank;

public class testeOficial {
public static void main(String[] args) {
	
	int day = 0;
 	double [] n = {1.30, 1.04, 2.5, 1.03, 1.08};
	
 	for(int counter = 0; counter < n.length; counter++ ) {
 		int soma = counter + 1;
 		
 		if(n[counter] + n[soma] <= 3) {
 			day++;
 		}
 		else {
 			break;
 		}
 		System.out.println(day);
 		
 	}
	
}	
}

